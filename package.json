  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist ./output && mkdirp ./dist ./output",
    "lint": "yarn run eslint \"**/**.[jt]s?(x)\"",
    "lint:report": "yarn run eslint \"**/**.[jt]s?(x)\" -o output/eslint.html -f html",
    "jest:config": "jest --config ./jest.config.js",
    "test": "yarn run jest:config",
    "ci": "yarn run clean; yarn run lint:report; yarn run test"
  },
